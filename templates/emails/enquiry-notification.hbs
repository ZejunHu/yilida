<h1>Hi YiLiDa</h1>
<p class="text-larger">An enquiry was just submitted to {{brand}}:</p>
{{#if enquiry.email}}
	<p class="text-larger">From
	{{#if enquiry.name.full}}
		<strong>{{enquiry.name.full}}</strong>
	{{/if}}

	{{#if enquiry.email}}
		(<a href="mailto:{{enquiry.email}}">{{enquiry.email}}</a>)
	{{/if}}
	</p>

	{{#if enquiry.subject}}
		<p class="text-larger">Phone: {{enquiry.subject}}</p>
	{{/if}}


	{{#if enquiry.message}}
		{{{enquiry.message.html}}}
	{{/if}}

	<p class="text-muted">Sent {{underscoreFormat enquiry 'createdAt'}}</p>
{{/if}}
